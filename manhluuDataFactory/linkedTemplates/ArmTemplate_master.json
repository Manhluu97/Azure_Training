{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "manhluuDataFactory"
		},
		"ConnectionManhDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ConnectionManhDB'"
		},
		"ConnectionPostgres_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ConnectionPostgres'"
		},
		"Connection_manhluudatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Connection_manhluudatalake'"
		},
		"Connection_manhluudatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://manhluudatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "manhluuDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ConnectionManhDB_connectionString": {
						"value": "[parameters('ConnectionManhDB_connectionString')]"
					},
					"Connection_manhluudatalake_accountKey": {
						"value": "[parameters('Connection_manhluudatalake_accountKey')]"
					},
					"ConnectionPostgres_connectionString": {
						"value": "[parameters('ConnectionPostgres_connectionString')]"
					},
					"Connection_manhluudatalake_properties_typeProperties_url": {
						"value": "[parameters('Connection_manhluudatalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}